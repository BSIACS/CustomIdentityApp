@model IEnumerable<Microsoft.AspNetCore.Identity.IdentityRole>
<table>
    @foreach (var role in Model) { 
        <tr>
            <td>@role.Name</td>
            <td><form asp-controller="Roles" asp-action="Delete" asp-route-id="@role.Id" method="post">
                <button type="submit" class="btn-sm btn-dark">Удалить</button>
                </form>
            </td>
        </tr>
    }
    
</table>